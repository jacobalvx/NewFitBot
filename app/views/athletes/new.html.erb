<h1> Create New Athlete</h1>

<%= form_for(@athlete, as: :athlete, url: athletes_path) do |u| %>
	<%=	u.label :name %>
	<%=	u.text_field :name %>
	<%=	u.label :email %>
	<%=	u.text_field :email %>
	<%=	u.label :password %>
	<%=	u.text_field :password %>
	<%=	u.label :weight %>
	<%=	u.number_field :weight %>
	<%=	u.label :fat %>
	<%=	u.number_field :muscle %>
	<%=	u.label :sport %>
	<%=	u.text_field :sport %>
	
	<%= u.submit "Create Athlete" %>
<% end %>



	